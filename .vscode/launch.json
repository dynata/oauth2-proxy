    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["--config", "contrib/local-environment/oauth2-proxy-pe-env-localhost.cfg"],
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}",
            "args": ["--config", "contrib/local-environment/oauth2-proxy-pe-env-localhost.cfg"]
        },
        {
            "name": "Attach remote",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 41800,
            "host": "127.0.0.1",
            "apiVersion": 2,
            "showLog": true
        }
    ]
}